{
    "name": "TaskManager",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "b": "pnpm --filter @taskmanager/backend ",
        "w": "pnpm --filter @taskmanager/webapp ",
        "dev": "pnpm -r --parallel dev",
