{
  "name": "@taskmanager/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "preview": "vite preview",
    "type": "tsc -b --noEmit",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "prettyfy": "prettier --log-level warn --cache --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.11",
    "@taskmanager/backend": "workspace:^",
    "@trpc/client": "^11.7.2",
    "@trpc/react-query": "^11.7.2",
    "formik": "^2.4.9",
    "include-media": "^2.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-modal": "^3.16.3",
    "react-router-dom": "^7.10.1",
    "reset-css": "^5.0.2",
    "sass": "^1.95.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-modal": "^3.13.1",
    "@vitejs/plugin-react": "^5.1.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.3",
    "stylelint": "^16.26.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4"
  }
}
